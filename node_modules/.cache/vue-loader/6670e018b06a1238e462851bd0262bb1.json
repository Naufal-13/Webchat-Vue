.from == 'admin'\" :key=\"index\">\r\n                  <div class=\"row\">\r\n                    <div class=\"col-2\">\r\n                      <small>{{item.date | changeDateFilter}}</small>\r\n                    </div>\r\n                      <div class=\"col\">\r\n                        <div class=\"kirim-warna\">\r\n                          {{item.msg}}\r\n                        </div>\r\n                      </div>\r\n                        <!-- Div untuk ceklist WA -->\r\n                      <div class=\"col-3\">\r\n                        <img src=\"../assets/logo-vutura.png\" class=\"image\">\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  \r\n                  \r\n                <div class=\"terima\" v-else :key=\"index\">\r\n                  <div class=\"row\">\r\n                    <div class=\"col-1\">\r\n                      <!-- <img src=\"../assets/avatar1.png\" class=\"image\"> -->\r\n                      <img :src=\"icon\" class=\"image\">\r\n                    </div>\r\n                    <div class=\"col\">\r\n                      <div class=\"terima-warna\">\r\n                        {{item.msg}}\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"col-2\">\r\n                      <small>{{item.date | changeDateFilter}}</small>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </template>\r\n            </div>\r\n\r\n            \r\n          \r\n          <!-- BAGIAN KIRIM-KIRIM -->\r\n            <div class=\"vgambar\" style=\"position: fixed\">\r\n                  <div style=\"position: fixed\">\r\n                    <img v-if=\"messageGambar\" :src=\"messageGambar\" width=\"300\" height=\"200\" />\r\n                  </div>\r\n            </div>\r\n            \r\n            <div id=\"message\">\r\n\r\n              <div class=\"row\" >\r\n                    \r\n                    <!-- Add Gambar -->\r\n                    <div class=\"col-md-1\">\r\n                      <div class=\"\">\r\n                        <label class=\"custom-file-upload\">\r\n                          <input type=\"file\" @change=\"viewGambar\"/>\r\n                          <i class=\"mdi mdi-36px mdi-image\"></i>\r\n                        </label>\r\n                      </div>\r\n                    </div>\r\n\r\n                    <!-- Tulis Pesan -->\r\n                  <div class=\"col-md-10\">\r\n                    <v-textarea outlined height=\"40px\" v-model=\"message\" name=\"pesan\" id=\"textarea1\" class=\"materialize-textarea\" cols=\"75\"\r\n                    placeholder=\"Masukkan Pesan...\" v-on:keyup.enter=\"kirim\"></v-textarea>\r\n                  </div>\r\n                  \r\n                  <!-- Tombol Kirim Pesan -->\r\n                  <div class=\"col\">\r\n                    <v-btn\r\n                    icon\r\n                    x-large\r\n                    color=\"black\"\r\n                    type=\"submit\"\r\n                    @click=\"kirim; kirimGambar\"\r\n                    >\r\n                      <v-icon >mdi-send</v-icon>\r\n                    </v-btn>\r\n                  </div>\r\n                  \r\n              </div>\r\n\r\n            </div>\r\n      </div>\r\n    </div>\r\n  </md-app>\r\n  </v-col>\r\n</template>\r\n\r\n\r\n<script>\r\nimport moment from 'moment'\r\nimport axios from 'axios'\r\nexport default {\r\n\r\n    data: () => ({\r\n    rr : sessionStorage.getItem('login'),\r\n    nama : '',\r\n    firstname : '',\r\n    profile : '',\r\n    message: null,\r\n    icon: null,\r\n    messageGambar: null,\r\n    item: 1,\r\n    user : [],\r\n    chat : [],\r\n    adadi : null,\r\n    date : '',\r\n  }),\r\n\r\n  methods: {\r\n\r\n    viewGambar(e){\r\n      const file = e.target.files[0];\r\n      this.messageGambar = URL.createObjectURL(file);\r\n    },\r\n\r\n    /* -------------- Ini untuk kirim chat  ----------- */\r\n    /* kirim: function() {\r\n      const id = sessionStorage.getItem('idkirimdata');\r\n      if(id){\r\n        axios.post('http://localhost:3000/admin/home/chat/'+id,{\r\n          message: this.message\r\n        })\r\n        .then((Response) =>{\r\n          if(Response.status == 201){\r\n            console.log(Response)\r\n            this.message = null;\r\n          }\r\n        })\r\n      }\r\n    }, */\r\n    kirim: function() {\r\n      const id = sessionStorage.getItem('idkirimdata');\r\n      if(this.message != null){\r\n        if(id){\r\n          axios.post('http://localhost:3000/admin/home/chat/'+id,{\r\n            message: this.message\r\n          })\r\n          .then((Response) =>{\r\n            if(Response.status == 201){\r\n              console.log(Response)\r\n              this.message = null;\r\n            }\r\n          })\r\n        }\r\n      }\r\n    },\r\n\r\n    /* -------------- Ini untuk kirim gambar  ----------- */\r\n    kirimGambar: function(){\r\n      const user = sessionStorage.getItem('idkirimdata');\r\n      if(user){\r\n        axios.post('ini endpointnya', {\r\n          messageGambar: this.messageGambar\r\n        })\r\n        .then((Response) =>{\r\n          if(Response.status == 200){ /* kodnya */\r\n            console.log(Response)\r\n            this.messageGambar = null;\r\n          } \r\n        })\r\n      }\r\n    },\r\n\r\n    /* -------------- Ini untuk Logout  ----------- */\r\n    logout: function(){\r\n      sessionStorage.clear();\r\n      window.location.href = \"http://localhost:8080\"\r\n    },\r\n\r\n    /* Getmsg id & username */\r\n    getMessage: function(s,u){\r\n      sessionStorage.removeItem('listChat')\r\n      this.chat = [];\r\n      this.date = '';\r\n      this.nama = '';\r\n      this.nama = u;\r\n      sessionStorage.removeItem('idkirimdata')\r\n      sessionStorage.setItem('idkirimdata',s);\r\n      sessionStorage.removeItem('listChat')\r\n      axios.get('http://localhost:3000/admin/home/'+s)\r\n      .then((r) => {\r\n        console.log(r.data.data)\r\n        this.date = r.data.data[0].date;\r\n        this.chat = r.data.data;\r\n        sessionStorage.setItem('listChat',JSON.stringify(r.data.data))\r\n      });\r\n    },\r\n\r\n    getIcon: function(x){\r\n      if(x==\"A\" || x==\"a\"){\r\n        this.icon = \"https://i.ibb.co/kx4bR4V/a.png\"\r\n      }\r\n      else if (x==\"B\" || x==\"b\"){\r\n        this.icon = \"https://i.ibb.co/72VdWzD/b.png\"\r\n      }\r\n      else if (x==\"C\" || x==\"c\"){\r\n        this.icon = \"https://i.ibb.co/48VsMPy/c.png\"\r\n      }\r\n      else if (x==\"D\" || x==\"d\"){\r\n        this.icon = \"https://i.ibb.co/S5FDtqQ/d.png\"\r\n      }\r\n      else if (x==\"E\" || x==\"e\"){\r\n        this.icon = \"https://i.ibb.co/mDG3yfd/e.png\"\r\n      }\r\n      else if (x==\"f\" || x==\"F\"){\r\n        this.icon = \"https://i.ibb.co/cbndMcz/f.png\"\r\n      }\r\n      else if (x==\"G\" || x==\"g\"){\r\n        this.icon = \"https://i.ibb.co/x2v3PVH/g.png\"\r\n      }\r\n      else if (x==\"H\" || x==\"h\"){\r\n        this.icon = \"https://i.ibb.co/rtZyBSd/h.png\"\r\n      }\r\n      else if (x==\"I\" || x==\"i\"){\r\n        this.icon = \"https://i.ibb.co/XkD2Qfc/i.png\"\r\n      }\r\n      else if (x==\"J\" || x==\"j\"){\r\n        this.icon = \"https://i.ibb.co/qNpXSTW/j.png\"\r\n      }\r\n      else if (x==\"K\" || x==\"k\"){\r\n        this.icon = \"https://i.ibb.co/QJdvM3b/k.png\"\r\n      }\r\n      else if (x==\"L\" || x==\"l\"){\r\n        this.icon = \"https://i.ibb.co/7gfYdFj/l.png\"\r\n      }\r\n      else if (x==\"M\" || x==\"m\"){\r\n        this.icon = \"https://i.ibb.co/bFByKxj/m.png\"\r\n      }\r\n      else if (x==\"N\" || x==\"n\"){\r\n        this.icon = \"https://i.ibb.co/mGb1GtK/n.png\"\r\n      }\r\n      else if (x==\"O\" || x==\"o\"){\r\n        this.icon = \"https://i.ibb.co/Hp80zyH/o.png\"\r\n      }\r\n      else if (x==\"P\" || x==\"p\"){\r\n        this.icon = \"https://i.ibb.co/RpYxrz9/p.png\"\r\n      }\r\n      else if (x==\"Q\" || x==\"q\"){\r\n        this.icon = \"https://i.ibb.co/tZkGcns/q.png\"\r\n      }\r\n      else if (x==\"R\" || x==\"r\"){\r\n        this.icon = \"https://i.ibb.co/f0HPw20/r.png\"\r\n      }\r\n      else if (x==\"S\" || x==\"s\"){\r\n        this.icon = \"https://i.ibb.co/pxBKBxB/s.png\"\r\n      }\r\n      else if (x==\"T\" || x==\"t\"){\r\n        this.icon = \"https://i.ibb.co/ZWjLYNX/t.png\"\r\n      }\r\n      else if (x==\"U\" || x==\"u\"){\r\n        this.icon = \"https://i.ibb.co/xXbzs0z/u.png\"\r\n      }\r\n      else if (x==\"V\" || x==\"v\"){\r\n        this.icon = \"https://i.ibb.co/XsBRsNZ/v.png\"\r\n      }\r\n      else if (x==\"W\" || x==\"w\"){\r\n        this.icon = \"https://i.ibb.co/PYCpzHN/w.png\"\r\n      }\r\n      else if (x==\"X\" || x==\"x\"){\r\n        this.icon = \"https://i.ibb.co/hfPwHDG/x.png\"\r\n      }\r\n      else if (x==\"Y\" || x==\"y\"){\r\n        this.icon = \"https://i.ibb.co/sJXFf8H/y.png\"\r\n      }\r\n      else if (x==\"Z\" || x==\"z\"){\r\n        this.icon = \"https://i.ibb.co/k5kMTtW/z.png\"\r\n      }\r\n    }\r\n  },\r\n\r\n  watch: {\r\n    firstname: function(newValue) {\r\n      this.r = this.getFirstLetter(newValue);\r\n    }\r\n  },\r\n\r\n  filters:{\r\n    changeDateFilter:\r\n    function (value){\r\n      return moment(value).format('LT');\r\n    },\r\n    changeDateFilterDate:\r\n    function (value){\r\n      return moment(value).format('L');\r\n    }\r\n  },\r\n\r\n  /* MELOAD ALL CUSTOMER */\r\n  mounted(){  \r\n    axios.get('http://localhost:3000/admin/home')\r\n    .then((r) => {\r\n      this.user = r.data.data;\r\n    });\r\n  }\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\ntextarea{\r\n  overflow: auto;\r\n  resize: none;\r\n}\r\n  .container\r\n  .app-wrapper {\r\n    min-height: 100vh;\r\n    position: absolute;\r\n    \r\n  }\r\n  .sidebar{\r\n    height: 100vh;\r\n    b