{"remainingRequest":"C:\\Users\\Naufal\\Desktop\\webchat\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Naufal\\Desktop\\webchat\\src\\views\\Chat.vue?vue&type=script&lang=js&","dependencies":[{"path":"C:\\Users\\Naufal\\Desktop\\webchat\\src\\views\\Chat.vue","mtime":1594020510461},{"path":"C:\\Users\\Naufal\\Desktop\\webchat\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Naufal\\Desktop\\webchat\\node_modules\\babel-loader\\lib\\index.js","mtime":499162500000},{"path":"C:\\Users\\Naufal\\Desktop\\webchat\\node_modules\\vuetify-loader\\lib\\loader.js","mtime":1574476662000},{"path":"C:\\Users\\Naufal\\Desktop\\webchat\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Naufal\\Desktop\\webchat\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Chat.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiMA;AACA;AACA;AACA;;AAEA;AACA;;;AAGA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA","file":"Chat.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n\r\n  <v-col cols=\"4\" sm=\"4\" md=\"12\">\r\n  <md-app :class=\"appWrapper\" class=\"app-wrapper\" >\r\n    <div class=\"container-fluid\" v-if=\"rr != null\">\r\n        <div class=\"sidebar\">\r\n          <div v-if=\"rb == 'admin'\">\r\n            <div id=\"\">\r\n              <v-btn\r\n                icon\r\n                x-large\r\n                color=\"white\"\r\n                disabled\r\n                id=\"chat\"\r\n              >\r\n                <v-icon dark>mdi-message-processing</v-icon>\r\n              </v-btn>\r\n            </div>\r\n\r\n            <div id=\"\">\r\n              <v-btn\r\n                icon\r\n                x-large\r\n                color=\"white\"\r\n                href=\"http://localhost:8080/agent\"\r\n                id=\"kontak\"\r\n              >\r\n                <v-icon dark>mdi-contacts</v-icon>\r\n              </v-btn>\r\n            </div>\r\n          </div>\r\n\r\n          <div id=\"logout\">\r\n            <v-btn\r\n              icon\r\n              x-large\r\n              color=\"white\"\r\n              @click=\"logout\"\r\n              id=\"keluar\"\r\n            >\r\n              <v-icon dark>mdi-login-variant</v-icon>\r\n            </v-btn>\r\n          \r\n          </div>\r\n        </div>\r\n\r\n      \r\n      <div id=\"listuser\" style=\"border-right-style: solid;  border-color: #C1C1C1\">\r\n        <div id=\"logo\">\r\n          <img src=\"../assets/logo-vutura.png\" id=\"logo-vutura\">\r\n        </div>\r\n\r\n        <!-- Perulangan user -->\r\n        <div id=\"boxuser\">\r\n          <div v-for=\"(item, index) in user\" :key=\"index\">\r\n            <div class=\"perulanganuser\" @click=\"getMessage(item._id, item.username); getIcon(item.username.charAt(0)); \">\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"4\" class=\"text-center\">\r\n                  <v-avatar color=\"#473390\">\r\n                    <span class=\"white--text headline text-capitalize\"> {{item.username.charAt(0)}} </span>\r\n                  </v-avatar>\r\n                </v-col>\r\n                 <v-col cols=\"12\" sm=\"8\">\r\n                  <p class=\"text-h6\" style=\"margin-top:7px;\"> {{item.username}} </p>\r\n                </v-col>\r\n              </v-row>\r\n            </div>\r\n          </div>\r\n        </div> \r\n      </div>\r\n\r\n      \r\n      <div id=\"boxchat\">\r\n          <div id=\"tglchat\" v-if=\"date\">{{date | changeDateFilterDate}}</div>\r\n\r\n          <div id=\"header-chat\">\r\n            <h1 style=\"margin: 15px 0 0 30px\">{{nama}} {{messageGambar}} {{pic}} </h1>\r\n          </div>\r\n\r\n          <div id=\"isichat\" v-if=\"chat != []\">\r\n            <div id=\"console-admin\"></div>\r\n              <!-- Chat yg dikirim ditampilkan -->\r\n              <template v-for=\"(item, index) in chat\">\r\n                \r\n                <!-- Dari damin -->\r\n                <div class=\"kirim\" v-if=\"item.from == 'admin'\" :key=\"index\">\r\n                  <div class=\"row\">\r\n                    <div class=\"col-2\">\r\n                      <small>{{item.date | changeDateFilter}}</small>\r\n                    </div>\r\n                      <div class=\"col\">\r\n                        <div v-if=\"item.msg\" class=\"kirim-warna\">\r\n                          {{item.msg}}\r\n                        </div>\r\n                        <div v-else class=\"kirim-warna\">\r\n                          <img :src=\"item.img\" width=\"300\" height=\"200\">\r\n                        </div>\r\n                      </div>\r\n                      <div class=\"col-3\">\r\n                        <img src=\"../assets/logo-vutura.png\" class=\"image\">\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  \r\n                <!-- Dari Customer -->\r\n                <div class=\"terima\" v-else :key=\"index\">\r\n                  <div class=\"row\">\r\n                    <div class=\"col-1\">\r\n                      <!-- <img src=\"../assets/avatar1.png\" class=\"image\"> -->\r\n                      <img :src=\"icon\" class=\"image\">\r\n                    </div>\r\n                    <div class=\"col\">\r\n                      <div class=\"terima-warna\">\r\n                        {{item.msg}}\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"col-2\">\r\n                      <small>{{item.date | changeDateFilter}}</small>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </template>\r\n            </div>\r\n\r\n            \r\n          \r\n          <!-- BAGIAN KIRIM-KIRIM -->\r\n            <div class=\"vgambar\" style=\"position: fixed\">\r\n                  <div style=\"position: fixed\">\r\n                    <img v-if=\"messageGambar\" :src=\"messageGambar\" width=\"300\" height=\"200\" />\r\n                  </div>\r\n            </div>\r\n            \r\n            <div id=\"message\">\r\n\r\n              <div class=\"row\" >\r\n                    \r\n                    <!-- Add Gambar -->\r\n                    <div class=\"col-md-1\">\r\n                      <div class=\"\">\r\n                        <label class=\"custom-file-upload\">\r\n                          <input type=\"file\" @change=\"viewGambar\"/>\r\n                          <i class=\"mdi mdi-36px mdi-image\"></i>\r\n                        </label>\r\n                      </div>\r\n                    </div>\r\n\r\n                    <!-- Tulis Pesan -->\r\n                    \r\n                  <div class=\"col-md-10\">\r\n                    <v-text-field outlined height=\"40px\" :disabled=\"messageGambar !== null\" v-model=\"message\" name=\"pesan\" id=\"textarea1\" class=\"materialize-textarea\" cols=\"75\"\r\n                    placeholder=\"Masukkan Pesan...\" @keydown=\"inputHandler\"></v-text-field>\r\n                  </div>\r\n                  \r\n                  <!-- Tombol Kirim Pesan -->\r\n                  <div class=\"col\">\r\n\r\n                    <v-btn\r\n                    v-if=\"message\"\r\n                    icon\r\n                    x-large\r\n                    color=\"black\"\r\n                    type=\"submit\"\r\n                    @click=\"sendMessage \"\r\n                    >\r\n                      <v-icon >mdi-send</v-icon>\r\n                    </v-btn>\r\n\r\n                    <v-btn\r\n                    v-if=\"messageGambar\"\r\n                    icon\r\n                    x-large\r\n                    color=\"black\"\r\n                    type=\"submit\"\r\n                    @click=\"sendPicture \"\r\n                    >\r\n                      <v-icon >mdi-send</v-icon>\r\n                    </v-btn>\r\n\r\n                  </div>\r\n                  \r\n              </div>\r\n\r\n            </div>\r\n      </div>\r\n    </div>\r\n  </md-app>\r\n  </v-col>\r\n</template>\r\n\r\n<script src=\"/socket.io/socket.io.js\"></script>\r\n\r\n<script>\r\nimport moment from 'moment'\r\nimport axios from 'axios'\r\nimport io from 'socket.io-client';\r\nimport firebase from 'firebase';\r\n\r\nvar socket = io('http://localhost:3000');   /* Koneksi */\r\nvar login = JSON.parse(localStorage.getItem(\"login\"));\r\n\r\n\r\nexport default {\r\n\r\n    data: () => ({\r\n    rr : sessionStorage.getItem('login'),\r\n    rb : sessionStorage.getItem('user'),\r\n    nama : '',\r\n    firstname : '',\r\n    profile : '',\r\n    message: '',\r\n    msg: null, /* untuk didalem fungsi aja */\r\n    img: null, /* untuk didalem fungsi aja */\r\n    idcust: null, /* untuk didalem fungsi aja */\r\n    icon: null,\r\n    messageGambar: null,\r\n    item: 1,\r\n    user : [],\r\n    cid: null, /* Menyimpan id cust yg dipencet */\r\n    chat : [], /* Penampung dari API */\r\n    chat1 : [], /* Penampung dari socket */\r\n    adadi : null,\r\n    date : '',\r\n    pic : null,\r\n    picUrl: null, /* nyimpen url hasil upload */\r\n    uploadValue: 0,\r\n  }),\r\n\r\n  /* Fungsi Socket */\r\n  created(){\r\n\r\n    /* Listen chat customer terpilih */       /* Masih Belum yakin */\r\n    socket.on('customerChat', (r) => {\r\n      if (cid == r.idcust){\r\n        this.chat1.push({\r\n          msg: r.msg,\r\n          from: 'customer',\r\n          img: r.img,\r\n        });\r\n      }\r\n    });\r\n  },  \r\n\r\n  methods: {\r\n\r\n    viewGambar(e){\r\n      const file = e.target.files[0];\r\n      this.pic = e.target.files[0];\r\n      this.messageGambar = URL.createObjectURL(file);\r\n    },\r\n\r\n    /* -------------- Ini untuk kirim chat API  ----------- */\r\n    sendMessage: function(event) {\r\n      const id = sessionStorage.getItem('idkirimdata');\r\n        if(this.message){\r\n          axios.post('http://localhost:3000/admin/home/chat/'+id,{\r\n            message: this.message\r\n          })\r\n          .then((Response) =>{\r\n            console.log(Response.status)\r\n            if(Response.status == 201){\r\n              console.log(Response)\r\n              this.chat.push({    /* langsung tampil di boxchat */\r\n                from: 'admin',\r\n                msg: this.message\r\n              })\r\n              console.log(this.chat)\r\n              this.message = '';\r\n            }\r\n          })\r\n        }\r\n    },\r\n\r\n    sendPicture: function(event) {\r\n      const id = sessionStorage.getItem('idkirimdata');\r\n      this.chat.push({\r\n        from: 'admin',\r\n        img: this.messageGambar\r\n      })\r\n      this.messageGambar = null\r\n    },\r\n\r\n    inputHandler: function(event){\r\n      if (event.keyCode === 13){\r\n        event.preventDefault()\r\n        if(this.message){\r\n          this.sendMessage()\r\n        }\r\n      }\r\n    },\r\n\r\n    /*-----Kirim Pesan dan gambar Dengan Socket-----*/\r\n    kirim: function() {\r\n      this.chat1.push({       /* Ini untuk live nampilin chat */\r\n        msg: this.message,\r\n        from: 'admin',\r\n        img: this.messageGambar,\r\n      });\r\n      const id = sessionStorage.getItem('idkirimdata');\r\n      socket.emit('adminChat', {  \r\n        msg: this.message,          /* Ini untuk emit chat ke socket */\r\n        idcust: id,\r\n        img: this.messageGambar,\r\n      });\r\n      this.message = null;\r\n      this.messageGambar = null;\r\n    },\r\n\r\n    /* -------------- Ini untuk Logout  ----------- */\r\n    logout: function(){\r\n      sessionStorage.clear();\r\n      window.location.href = \"http://localhost:8080\"\r\n    },\r\n\r\n    /* Get msg id & username API (klik user) */\r\n    getMessage: function(s,u){\r\n      sessionStorage.removeItem('listChat')\r\n      \r\n      this.chat = [];\r\n      this.date = '';\r\n      this.nama = '';\r\n      this.nama = u;\r\n      this.cid = s; /* Menyimpan ID yg dipencet */\r\n      sessionStorage.removeItem('idkirimdata')\r\n      sessionStorage.setItem('idkirimdata',s);\r\n      sessionStorage.removeItem('listChat')\r\n      axios.get('http://localhost:3000/admin/home/'+s)\r\n      .then((r) => {\r\n        console.log(r.data.data)\r\n        this.date = r.data.data[0].date;\r\n        this.chat = r.data.data;\r\n        sessionStorage.setItem('listChat',JSON.stringify(r.data.data))\r\n        socket.emit('register-as-admin',login.id);\r\n      });\r\n    },\r\n\r\n    /* untuk menentukan icon customer*/\r\n    getIcon: function(x){\r\n      if(x==\"A\" || x==\"a\"){\r\n        this.icon = \"https://i.ibb.co/kx4bR4V/a.png\"\r\n      }\r\n      else if (x==\"B\" || x==\"b\"){\r\n        this.icon = \"https://i.ibb.co/72VdWzD/b.png\"\r\n      }\r\n      else if (x==\"C\" || x==\"c\"){\r\n        this.icon = \"https://i.ibb.co/48VsMPy/c.png\"\r\n      }\r\n      else if (x==\"D\" || x==\"d\"){\r\n        this.icon = \"https://i.ibb.co/S5FDtqQ/d.png\"\r\n      }\r\n      else if (x==\"E\" || x==\"e\"){\r\n        this.icon = \"https://i.ibb.co/mDG3yfd/e.png\"\r\n      }\r\n      else if (x==\"f\" || x==\"F\"){\r\n        this.icon = \"https://i.ibb.co/cbndMcz/f.png\"\r\n      }\r\n      else if (x==\"G\" || x==\"g\"){\r\n        this.icon = \"https://i.ibb.co/x2v3PVH/g.png\"\r\n      }\r\n      else if (x==\"H\" || x==\"h\"){\r\n        this.icon = \"https://i.ibb.co/rtZyBSd/h.png\"\r\n      }\r\n      else if (x==\"I\" || x==\"i\"){\r\n        this.icon = \"https://i.ibb.co/XkD2Qfc/i.png\"\r\n      }\r\n      else if (x==\"J\" || x==\"j\"){\r\n        this.icon = \"https://i.ibb.co/qNpXSTW/j.png\"\r\n      }\r\n      else if (x==\"K\" || x==\"k\"){\r\n        this.icon = \"https://i.ibb.co/QJdvM3b/k.png\"\r\n      }\r\n      else if (x==\"L\" || x==\"l\"){\r\n        this.icon = \"https://i.ibb.co/7gfYdFj/l.png\"\r\n      }\r\n      else if (x==\"M\" || x==\"m\"){\r\n        this.icon = \"https://i.ibb.co/bFByKxj/m.png\"\r\n      }\r\n      else if (x==\"N\" || x==\"n\"){\r\n        this.icon = \"https://i.ibb.co/mGb1GtK/n.png\"\r\n      }\r\n      else if (x==\"O\" || x==\"o\"){\r\n        this.icon = \"https://i.ibb.co/Hp80zyH/o.png\"\r\n      }\r\n      else if (x==\"P\" || x==\"p\"){\r\n        this.icon = \"https://i.ibb.co/RpYxrz9/p.png\"\r\n      }\r\n      else if (x==\"Q\" || x==\"q\"){\r\n        this.icon = \"https://i.ibb.co/tZkGcns/q.png\"\r\n      }\r\n      else if (x==\"R\" || x==\"r\"){\r\n        this.icon = \"https://i.ibb.co/f0HPw20/r.png\"\r\n      }\r\n      else if (x==\"S\" || x==\"s