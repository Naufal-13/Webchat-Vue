 <div id=\"boxuser\">\r\n          <template v-for=\"(item, index) in user\">\r\n            <div v-if=\"user\" :key=\"index\" >\r\n            <div class=\"perulanganuser\" @click=\"getMessage(item._id)\">\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"4\" class=\"text-center\">\r\n                  <img src=\"../assets/avatar1.png\" class=\"image\">\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"8\">\r\n                  <p class=\"text-h6\" style=\"margin-top:7px;\"> {{item.username}} </p>\r\n                </v-col>\r\n              </v-row>\r\n            </div>\r\n            </div>\r\n          </template>\r\n\r\n\r\n        </div> \r\n    </div>\r\n\r\n      \r\n\r\n\r\n\r\n      <div id=\"boxchat\">\r\n          <div id=\"tglchat\">20 Desember 2020</div>\r\n\r\n          <div id=\"header-chat\">\r\n            <p class=\"text-h5\" style=\"margin: 15px 0 0 30px\">chalik</p>\r\n          </div>\r\n\r\n          <div id=\"isichat\">\r\n            <div id=\"console-admin\"></div>\r\n              <!-- Chat yg dikirim ditampilkan -->\r\n              <template v-for=\"(item, index) in chat\">\r\n                <div class=\"kirim\" v-if=\"item.from == 'admin'\" :key=\"index\">\r\n                  <div class=\"row\">\r\n                    <div class=\"col-2\">\r\n                      <small>{{item.date | changeDateFilter}}</small>\r\n                    </div>\r\n                      <div class=\"col\">\r\n                        <div class=\"kirim-warna\">\r\n                          {{item.msg}}\r\n                        </div>\r\n                      </div>\r\n                      <div class=\"col-3\">\r\n                        <img src=\"../assets/logo-vutura.png\" class=\"image\">\r\n                      </div>\r\n                    </div>\r\n                </div>\r\n                <div class=\"terima\" v-else :key=\"index\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-1\">\r\n                    <img src=\"../assets/avatar1.png\" class=\"image\">\r\n                  </div>\r\n                  <div class=\"col\">\r\n                    <div class=\"terima-warna\">\r\n                      {{item.msg}}\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-2\">\r\n                    <small>{{item.date | changeDateFilter}}</small>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              </template>\r\n            </div>\r\n\r\n            \r\n          \r\n\r\n            <div id=\"message\">\r\n              <div class=\"row\">\r\n                  <div class=\"col\">\r\n                    <textarea v-model=\"message\" name=\"pesan\" id=\"textarea1\" class=\"materialize-textarea\" cols=\"75\"\r\n                    placeholder=\"Masukkan Pesan...\"></textarea>\r\n                  </div>\r\n                  <div class=\"col\">\r\n                    <v-btn\r\n                    large\r\n                    dark\r\n                    type=\"submit\"\r\n                    height=\"50px\"\r\n                    @click=\"kirim\"\r\n                    >\r\n                      <v-icon >mdi-send</v-icon>\r\n                    </v-btn>\r\n                  </div>\r\n              </div>\r\n            </div>\r\n      </div>\r\n    </div>\r\n  </md-app>\r\n</template>\r\n\r\n\r\n<script>\r\nimport moment from 'moment'\r\nimport axios from 'axios'\r\naxios.get('http://localhost:3000/admin/home')\r\n.then((r) => {\r\n  const user = r.data.data;\r\n  sessionStorage.setItem('userChat', JSON.stringify(user))\r\n  console.log(JSON.parse(sessionStorage.getItem('userChat')))\r\n});\r\nexport default {\r\n\r\n    data: () => ({\r\n    dataChat: [],\r\n    message: null,\r\n    item: 1,\r\n\r\n    /* -------------- Ini untuk daftar user  ----------- */\r\n    user: JSON.parse(sessionStorage.getItem('userChat')),\r\n    /* -------------- Ini untuk history chat  ----------- */\r\n    chat: JSON.parse(sessionStorage.getItem('listChat'))\r\n    \r\n  }),\r\n\r\n  methods: {\r\n    /* -------------- Ini untuk kirim chat  ----------- */\r\n    kirim() {\r\n      this.dataChat.push(this.message);\r\n      axios.post('http://localhost:3000/admin/home/chat/--id customer yg dipencet--',this.message)\r\n      this.message = \"\";\r\n    },\r\n    getMessage: function(s){\r\n      axios.get('http://localhost:3000/admin/home/'+s)\r\n      .then((r) => {\r\n        const user = r.data.data; \r\n        sessionStorage.setItem('listChat', JSON.stringify(user))\r\n        \r\n      });\r\n    },\r\n  },\r\n  filters:{\r\n    changeDateFilter:\r\n    function (value){\r\n      return moment(value).format('LT');\r\n    }\r\n  }\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\ntextarea{\r\n  overflow: auto;\r\n  resize: none;\r\n}\r\n  .container\r\n  .app-wrapper {\r\n    min-height: 100vh;\r\n    position: absolute;\r\n    \r\n  }\r\n  .sidebar{\r\n    height: 100vh;\r\n    background-color: #473390;\r\n    width: 70px;\r\n    box-shadow: 0 0 10px 0;\r\n    position: absolute;\r\n    z-index: 1;\r\n  }\r\n  #listuser{\r\n    background-color: #F1EFFF;\r\n    position: absolute;\r\n    left: 70px;\r\n    z-index: 0;\r\n    width: 438px;\r\n    height: 100%;\r\n  }\r\n  #boxchat{\r\n    background-color: #FFFFFF;\r\n    position: absolute;\r\n    left: 507px;\r\n    z-index: 0;\r\n    width: 67%;\r\n    height: 100%;\r\n  }\r\n  #boxuser{\r\n    position: absolute;\r\n    background-color: #F1EFFF;\r\n    top: 70px;\r\n    width: 100%;\r\n    height: 80%;\r\n    box-shadow: 0 -5px 5px -5px rgb(214, 214, 214);\r\n    overflow-x: hidden;\r\n    overflow-y: auto;\r\n  }\r\n  \r\n  .perulanganuser{\r\n    width: 100%;\r\n    height: 60px;\r\n    position: static;\r\n    background-color: #F1EFFF;\r\n    margin-top: 2px;\r\n    box-shadow: 0 5px 10px -10px #333;\r\n  }\r\n  .perulanganuser:hover,\r\n  .perulanganuser:focus {\r\n      text-decoration: none;\r\n      cursor: pointer;\r\n      background-color: #DEDAFF;\r\n  }\r\n\r\n  #header-chat{\r\n    background-color: #F1EFFF;\r\n    height: 70px;\r\n    width: 100%;\r\n    position: absolute;\r\n    top: 0px;\r\n  }\r\n  #isichat{\r\n    position: absolute;\r\n    top:110px;\r\n    width: 100%;\r\n    padding: 10px 0px 30px 10px;\r\n    background-color: white;\r\n    height: 70%;\r\n    overflow-x: hidden;\r\n    overflow-y: auto;\r\n  }\r\n  .terima{\r\n    padding: 5px 0 0 10px;\r\n    float: left;\r\n    width: 80%;\r\n  }\r\n   .kirim{\r\n    padding: 5px 0 0 10px;\r\n    float: right;\r\n    position: relative;\r\n\r\n    width: 80%;\r\n  }\r\n  #console {\r\n    width: 100%;\r\n  }\r\n  #console-admin {\r\n    width: 100%;\r\n  }\r\n  .terima-warna{\r\n    word-break: break-word;\r\n    border-radius: 0px 15px 15px 15px;\r\n    padding: 10px 20px;\r\n    background-color: #F1EFFF;\r\n    position: static;\r\n    text-align: left;\r\n    width: 100%;\r\n    max-width: 700px;\r\n  }\r\n  .kirim-warna{\r\n    word-break: break-word;\r\n    border-radius: 0px 15px 15px 15px;\r\n    padding: 10px 20px;\r\n    background-color: #F1EFFF;\r\n    position: static;\r\n    text-align: right;\r\n    width: 100%;\r\n    max-width: 700px;\r\n  }\r\n  #message{\r\n    position: fixed;\r\n    bottom: 0px;\r\n    height: 15%;\r\n    width: 100%;\r\n    background-color: #F1EFFF;\r\n  }\r\n  #keluar{\r\n    bottom: 12px;\r\n    position: absolute;\r\n    left: 12px;\r\n  }\r\n  #logo-vutura{\r\n    height: 70px;\r\n    position: absolute;\r\n    padding: 10px;\r\n    right: 45%;\r\n  }\r\n  .image{\r\n    width: 40px;\r\n    border-radius: 30px;\r\n  }\r\n  #tglchat{\r\n    position: absolute;\r\n    top: 80px;\r\n    background-color: #F1EFFF;\r\n    margin: 0 -50% 0 40%;\r\n    padding: 5px;\r\n    border-radius: 5px;\r\n  }\r\n</style>\r\n"]}]}