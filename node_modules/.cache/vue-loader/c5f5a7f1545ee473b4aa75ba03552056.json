\n                      <th class=\"text-left\"></th>\r\n                    </tr>\r\n                  </thead>\r\n                  <tbody>\r\n                    \r\n                    <tr>\r\n                      <td>Anggur</td>\r\n                      <td>Anggur@gmail.com</td>\r\n                      <td>Anggur_7980XE</td>\r\n                      <td>\r\n                        <v-btn icon> <v-icon>mdi-pencil</v-icon> </v-btn>\r\n                        <v-btn icon> <v-icon>mdi-delete</v-icon> </v-btn>\r\n                      </td>\r\n                    </tr>\r\n                    \r\n\r\n                    <!-- LOOPING ALL AGENT -->\r\n                    <template v-for=\"(item, index) in agent\">\r\n                        <tr v-if=\"agent\" :key=\"index\">\r\n                          <td> {{item.nama}} </td>\r\n                          <td> {{item.email}} </td>\r\n                          <td> {{item.password}} </td>\r\n                          <td>\r\n                            <v-btn icon @click=\"getAkun\"> <v-icon>mdi-pencil</v-icon> </v-btn>\r\n                            <v-btn icon> <v-icon>mdi-delete</v-icon> </v-btn>\r\n                          </td>\r\n                        </tr>\r\n                    </template>\r\n\r\n                  </tbody>\r\n                </template>\r\n              </v-simple-table>\r\n            </div>\r\n          </template>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Bagian Edit Agent -->\r\n      <div id=\"boxeditagent1\">\r\n        <div style=\"margin: 80px\">\r\n          <v-card class=\"mx-auto\" width=\"1000\" height=\"800\">\r\n            <v-card-text>\r\n              <div style=\"padding-top: 30px\"></div>\r\n              <!-- Header -->\r\n              <div style=\"text-align: center \">\r\n                <p class=\"display-1 text--primary\"> EDIT AGENT</p>\r\n              </div>\r\n              \r\n              <!-- Isi -->\r\n              <div style=\"padding-top: 90px\"></div>\r\n                <div style=\"<!-- margin-left: 100px --> \">\r\n                  <div class=\"isian\">\r\n                    <div style=\"text-align: center \">\r\n                      <h3> NAMA</h3>\r\n                        <v-text-field\r\n                          v-model=\"nama\"\r\n                          width=\"200px\"\r\n                          placeholder=\"Tuliskan Nama\"\r\n                        >\r\n                        </v-text-field>\r\n                    </div>\r\n                    <div style=\"padding-top: 30px\"></div>\r\n                    <div style=\"text-align: center \">\r\n                      <h3> E-MAIL</h3>\r\n                      <v-text-field\r\n                        v-model=\"email\"\r\n                        width=\"200px\"\r\n                        placeholder=\"Tuliskan Email\"\r\n                      >\r\n                      </v-text-field>\r\n                    </div>\r\n                    <div style=\"padding-top: 30px\"></div>\r\n                    <div style=\"text-align: center \">\r\n                      <h3> PASSWORD</h3>\r\n                      <v-text-field\r\n                        v-model=\"password\"\r\n                        width=\"200px\"\r\n                        placeholder=\"Tuliskan Passsword\"\r\n                      >\r\n                      </v-text-field>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n            </v-card-text>\r\n\r\n            <!-- Tombol -->\r\n            <div style=\"padding-top: 90px\"></div>\r\n            <div style=\"text-align: center \"> {{warning}} </div>\r\n            <div style=\"text-align: center \">\r\n              <v-btn \r\n                dark \r\n                color=\"#FF6C00\"\r\n                flat\r\n                width=\"200px\"\r\n                height=\"50px\"\r\n                @click=\"simpan\"\r\n              >\r\n              SIMPAN\r\n              </v-btn>\r\n            </div>\r\n        \r\n          </v-card>\r\n        </div>\r\n      </div>\r\n      \r\n      \r\n    </div>\r\n  </md-app>\r\n  </v-col>\r\n</template>\r\n\r\n\r\n<script>\r\nimport moment from 'moment'\r\nimport axios from 'axios'\r\nexport default {\r\n\r\n    data: () => ({\r\n    rr : sessionStorage.getItem('login'),\r\n    nama : '',\r\n    email : '',\r\n    password: '',\r\n    message: null,\r\n    warning: null,\r\n    messageGambar: null,\r\n    item: 1,\r\n    agent : [\r\n      {nama: 'Adlin', email: 'torq@gmail.com', password: 'touring'},\r\n      {nama: 'Pewd', email: 'tor@ymail.com', password: 'GRANTOURER'},\r\n      {nama: 'Von', email: 'zyl@gmail.com', password: 'layz'},\r\n    ],\r\n    chat : [],\r\n    adadi : null,\r\n    date : '',\r\n  }),\r\n\r\n  methods: {\r\n    /* -------------- Ini untuk kirim chat  ----------- */\r\n    kirim: function() {\r\n      const id = sessionStorage.getItem('idkirimdata');\r\n      if(id){\r\n        axios.post('http://localhost:3000/admin/home/chat/'+id,{\r\n          message: this.message\r\n        })\r\n        .then((Response) =>{\r\n          if(Response.status == 201){\r\n            console.log(Response)\r\n            this.message = null;\r\n          }\r\n        })\r\n      }\r\n    },\r\n\r\n    /* CREATE AGENT */\r\n    simpan: function() {\r\n      axios.post('ENDPOINTNYA',{\r\n        nama: this.nama,\r\n        email: this.email,\r\n        password: this.password\r\n      })\r\n      .then((Response) =>{\r\n          if(Response.status == 201){\r\n            console.log(Response)\r\n            this.warning = null;\r\n          }\r\n          else{\r\n            this.warning = \"Gagal\";\r\n          }\r\n        })\r\n    },\r\n\r\n    /* EDIT AGENT */\r\n    ubah: function() {\r\n      axios.post('ENDPOINTNYA',{\r\n        nama: this.nama,\r\n        email: this.email,\r\n        password: this.password\r\n      })\r\n      .then((Response) =>{\r\n          if(Response.status == 201){\r\n            console.log(Response)\r\n            this.warning = null;\r\n          }\r\n          else{\r\n            this.warning = \"Gagal\";\r\n          }\r\n        })\r\n    },\r\n\r\n    /* Get Agent */\r\n    getAkun: function(){\r\n\r\n    },\r\n\r\n    /* Hapus Agent */\r\n    hapus: function(){\r\n\r\n    },\r\n\r\n    \r\n\r\n    /* -------------- Ini untuk Logout  ----------- */\r\n    logout: function(){\r\n      sessionStorage.clear();\r\n      window.location.href = \"http://localhost:8080\"\r\n    },\r\n\r\n  },\r\n  filters:{\r\n    changeDateFilter:\r\n    function (value){\r\n      return moment(value).format('LT');\r\n    },\r\n    changeDateFilterDate:\r\n    function (value){\r\n      return moment(value).format('L');\r\n    }\r\n  },\r\n \r\n  /* MELOAD SELURUH AGENT */\r\n  mounted(){\r\n    axios.get('----------') /* ENDPOINT BELUM ADA */\r\n    .then((r) => {\r\n      this.agent = r.data.data;\r\n    });\r\n  }\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\ntextarea{\r\n  overflow: auto;\r\n  resize: none;\r\n}\r\n  .container\r\n  .app-wrapper {\r\n    min-height: 100vh;\r\n    position: absolute;\r\n    \r\n  }\r\n  .sidebar1{\r\n    height: 100vh;\r\n    background-color: #473390;\r\n    width: 70px;\r\n    box-shadow: 0 0 10px 0;\r\n    position: absolute;\r\n    z-index: 1;\r\n  }\r\n  #listuser1{\r\n    background-color: #F1EFFF;\r\n    position: absolute;\r\n    left: 70px;\r\n    z-index: 0;\r\n    width: 900px;\r\n    height: 100%;\r\n  }\r\n  #warnavutura{\r\n    background-color: #F1EFFF;\r\n  }\r\n  #boxchat1{\r\n    background-color: #FFFFFF;\r\n    position: absolute;\r\n    left: 800px;\r\n    z-index: 0;\r\n    width: 67%;\r\n    height: 100%;\r\n  }\r\n  #boxeditagent1{\r\n    background-color: white;\r\n    position: absolute;\r\n    left: 970px;\r\n    z-index: 0;\r\n    width: 40%;\r\n    height: 100%;\r\n  }\r\n  #boxuser1{\r\n    position: absolute;\r\n    background-color: #F1EFFF;\r\n    top: 70px;\r\n    width: 150%;\r\n    height: 80%;\r\n    box-shadow: 0 -5px 5px -5px rgb(214, 214, 214);\r\n    overflow-x: hidden;\r\n    overflow-y: auto;\r\n  }\r\n  \r\n  .perulanganuser1{\r\n    width: 100%;\r\n    height: 60px;\r\n    position: static;\r\n    background-color: #F1EFFF;\r\n    margin-top: 2px;\r\n    box-shadow: 0 5px 10px -10px #333;\r\n  }\r\n  .perulanganuser:hover,\r\n  .perulanganuser:focus {\r\n      text-decoration: none;\r\n      cursor: pointer;\r\n      background-color: #DEDAFF;\r\n  }\r\n\r\n  #header-chat{\r\n    background-color: #F1EFFF;\r\n    height: 70px;\r\n    width: 100%;\r\n    position: absolute;\r\n    top: 0px;\r\n  }\r\n  #isichat{\r\n    position: absolute;\r\n    top:110px;\r\n    width: 100%;\r\n    padding: 10px 0px 30px 10px;\r\n    background-color: white;\r\n    height: 70%;\r\n    overflow-x: hidden;\r\n    overflow-y: auto;\r\n  }\r\n  .terima{\r\n    padding: 5px 0 0 10px;\r\n    float: left;\r\n    width: 80%;\r\n  }\r\n   .kirim{\r\n    padding: 5px 0 0 10px;\r\n    float: right;\r\n    position: relative;\r\n    width: 80%;\r\n  }\r\n  #console {\r\n    width: 100%;\r\n  }\r\n  #console-admin {\r\n    width: 100%;\r\n  }\r\n  .terima-warna{\r\n    word-break: break-word;\r\n    border-radius: 0px 15px 15px 15px;\r\n    padding: 10px 20px;\r\n    background-color: #F1EFFF;\r\n    position: static;\r\n    text-align: left;\r\n    width: 100%;\r\n    max-width: 700px;\r\n  }\r\n  .kirim-warna{\r\n    word-break: break-word;\r\n    border-radius: 15px 0px 15px 15px;\r\n    padding: 10px 20px;\r\n    background-color: #C0B7FF;\r\n    position: static;\r\n    text-align: right;\r\n    width: 100%;\r\n    max-width: 700px;\r\n  }\r\n  \r\n  #keluar{\r\n    bottom: 12px;\r\n    position: absolute;\r\n    left: 12px;\r\n  }\r\n  #chat{\r\n    top: 80px;\r\n    position: absolute;\r\n    left: 12px;\r\n  }\r\n  #kontak{\r\n    top: 150px;\r\n    position: absolute;\r\n    left: 12px;\r\n  }\r\n  #logo-vutura{\r\n    height: 70px;\r\n    position: absolute;\r\n    padding: 10px;\r\n    right: 45%;\r\n  }\r\n  .image{\r\n    width: 40px;\r\n    border-radius: 30px;\r\n  }\r\n  #tglchat{\r\n    position: absolute;\r\n    top: 80px;\r\n    background-color: #F1EFFF;\r\n    margin: 0 -50% 0 40%;\r\n    padding: 5px;\r\n    border-radius: 5px;\r\n  }\r\n  .isian{\r\n    margin-right: 100px;\r\n    margin-left: 100px;\r\n  }\r\n</style>\r\n"]}]}