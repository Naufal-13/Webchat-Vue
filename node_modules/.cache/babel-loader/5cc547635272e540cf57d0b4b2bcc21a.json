     >\r\n                  \r\n                  <v-spacer></v-spacer>\r\n                  <div id=\"logo\">\r\n                    <img src=\"../assets/logo-vutura.png\" id=\"logo-vutura\">\r\n                  </div>\r\n                  <v-spacer></v-spacer>\r\n                  \r\n\r\n                </v-toolbar>\r\n\r\n                <div style=\"padding-top:0.5cm\"></div>\r\n                  \r\n\r\n                <v-card-text>\r\n                  \r\n                  <v-form\r\n                    ref=\"form\"\r\n                    v-model=\"valid\"\r\n                    lazy-validation\r\n                  >\r\n                    <div style=\"padding-top:1cm\"></div>\r\n                    <v-toolbar flat color=\"#F1EFFF\">\r\n                      <v-spacer></v-spacer>\r\n                      <v-text-field\r\n                        v-model=\"username\"\r\n                        label=\"Username\"\r\n                        :rules=\"usernameRules\"\r\n                        name=\"username\"\r\n                        solo\r\n                        flat\r\n                        class=\"my-input\"\r\n                      ></v-text-field>\r\n                      <v-spacer></v-spacer> \r\n                    </v-toolbar>\r\n\r\n                    <div style=\"padding-top:0.5cm\"></div>\r\n\r\n                    <v-toolbar flat color=\"#F1EFFF\"> \r\n                      <v-spacer></v-spacer> \r\n                    <v-text-field\r\n                      v-model=\"password\"\r\n                      id=\"password\"\r\n                      label=\"Password\"\r\n                      :rules=\"passwordRules\"\r\n                      v-on:keyup.enter=\"login\"\r\n                      name=\"password\"\r\n                      solo\r\n                      flat\r\n                      class=\"my-input\"\r\n                      type=\"password\"\r\n                    ></v-text-field>\r\n                    <v-spacer></v-spacer> \r\n                    </v-toolbar>\r\n\r\n                  </v-form>\r\n                \r\n                <!-- Ini untuk nampilin pesan error -->\r\n                </v-card-text>\r\n                  <v-alert type=\"error\" \r\n                    :value=\"alert\"\r\n                  >\r\n                    {{pesanError}}\r\n                  </v-alert>\r\n                <v-card-actions>\r\n                  \r\n                  <v-spacer></v-spacer>\r\n                  <v-btn \r\n                    dark \r\n                    color=\"#FF6C00\"\r\n                    @click=\"login\"\r\n                    flat\r\n                    width=\"200px\"\r\n                    height=\"50px\"\r\n                  >\r\n                  Login\r\n                  </v-btn>\r\n                  <v-spacer></v-spacer>\r\n                </v-card-actions>\r\n\r\n                  <v-layout row v-if=\"error\">\r\n                    <v-flex xs12 sm6 offset-sm3>\r\n                      <app-alert @dismissed=\"onDismissed\" :text=\"error.message\"></app-alert>\r\n                    </v-flex>\r\n                  </v-layout>\r\n\r\n                  <div style=\"padding-bottom:1cm\"></div>\r\n              </v-card>\r\n            </v-col>\r\n          </v-row>\r\n        </v-container>\r\n\r\n    </template>\r\n    <!--------------End LOGIN----------------------------------------->\r\n    </v-app>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  data: () => ({\r\n    valid: true,\r\n    username: '',\r\n    password: '',\r\n    pesanError: '',\r\n    alert: false,\r\n    usernameRules: [\r\n      v => !!v || 'Masukan Username',\r\n    ],\r\n    passwordRules: [\r\n      v => !!v || 'Masukan Password',\r\n    ],\r\n  }),\r\n\r\n  methods: {\r\n    login: function(){\r\n      axios.post('http://localhost:3000/admin/login',{\r\n          username: this.username,\r\n          password: this.password\r\n        })\r\n        .then((Response) =>{\r\n          const log = Response.data;\r\n          sessionStorage.setItem('login',log.id);\r\n          if(log.msg == \"Login Sukses\"){\r\n            axios.get('http://localhost:3000/admin/home')\r\n          .then((r) => {\r\n            const user = r.data.data;\r\n            sessionStorage.setItem('userChat', JSON.stringify(user))\r\n            console.log(JSON.parse(sessionStorage.getItem('userChat')))\r\n          });\r\n            window.location.href = \"http://localhost:8080/Chat\"\r\n          }\r\n          else if (log.msg != \"Login Sukses\"){\r\n            this.pesanError= \"Username atau Password Salah\";\r\n            this.alert= true;\r\n          }\r\n        })\r\n        .catch((err) =>{\r\n          /* untuk error 4xx */\r\n          if (err.response){\r\n            this.pesanError= \"Username atau Password Salah\";\r\n            this.alert= true;\r\n          }\r\n          /* misal req error dll */\r\n          else{\r\n            this.pesanError= \"Sistem Sedang Gangguan\";\r\n            this.alert= true;\r\n          }\r\n        })\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n.my-input.v-input .v-input__slot {\r\n  border-radius: 100px;\r\n  width: auto;\r\n}\r\n\r\n.tengah{\r\n  text-align: center;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}